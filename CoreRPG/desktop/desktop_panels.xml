<root>
	<panel name="desktopdecalpanel">
		<class>desktopdecal</class>
		<bounds>100,100,-100,-100</bounds>
		<disabled />
	</panel>

	<panel name="shortcutsanchor">
		<class>shortcutsanchor</class>
		<bounds>-304,0,0,0</bounds>
		<disabled />
	</panel>
	<panel name="shortcuts">
		<class>shortcuts</class>
		<anchored>
			<top />
			<left parent="shortcutsanchor" anchor="right" />
			<right />
			<bottom offset="-33" />
		</anchored>
	</panel>

	<panel name="imagebackpanel">
		<anchored>
			<left offset="395" /> 
			<top />
			<right parent="shortcutsanchor" anchor="left" />
			<bottom offset="-33" />
		</anchored>
		<class>imagebackpanel</class>
		<disabled />
	</panel>

	<panel name="chat">
		<anchored width="385">
			<left offset="5" />
			<top offset="120" />
			<bottom offset="-110" />
		</anchored>
		<class>chat</class>
		<dynamic />
		<locked />
	</panel>

	<panel name="modifierstack">
		<anchored width="64" height="57">
			<left offset="5" />
			<top anchor="bottom" offset="-100" />
		</anchored>
		<class>modifierstack</class>
		<dynamic />
		<locked />
	</panel>

	<panel name="dicetower">
		<anchored height="90" width="90">
			<bottom offset="-35" />
			<right parent="shortcutsanchor" anchor="left" offset="-45" />
		</anchored>
		<class>dicetower</class>
		<dynamic />
		<locked />
	</panel>

	<panel name="imagefullpanel">
		<anchored>
			<left /> 
			<top />
			<right />
			<bottom offset="-33" />
		</anchored>
		<class>imagefullpanel</class>
		<disabled />
	</panel>

	<panel name="dicepanel">
		<class>dicepanel</class>
		<anchored width="50" height="50">
			<top anchor="bottom" offset="-105" />
			<left offset="75" />
		</anchored>
		<dynamic />
		<locked />
	</panel>

	<panel name="characterlist">
		<anchored height="120" width="40">
			<left />
			<top />
		</anchored>
		<class>characterlist</class>
		<dynamic />
		<locked />
	</panel>
</root>